{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "enabledManagers": ["custom.regex"],
  "customManagers": [{
    "customType": "regex",
    "fileMatch": ["(^|/)cdktf\\.json$"],
    "matchStrings": [
      "[\\s\\S]*?\\\"name\\\": \\\"(?<depName>.*?)\\\",[\\s\\S]*?\\\"source\\\": \\\"(?<sourceUrl>(?:\\w+\\.\\w+){1,}\\/)?(?<packageName>[^\\.]*?)\\\",[\\s\\S]*?\\\"version\\\": \\\"(?<currentValue>[=><~\\s\\w+\\.\\-]*)\\\""
    ],
    "datasourceTemplate": "terraform-provider"
  }]
}